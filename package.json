{
  "name": "my-platform",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "nest start --watch",
    "dev:frontend": "cd come-admin-panel-auth-base && npm run dev",
    "build": "nest build && cd come-admin-panel-auth-base && npm run build",
    "start": "cd come-admin-panel-auth-base && npm run start",
    "start:backend": "node dist/main",
    "start:prod": "npm run build && npm run start:backend",
    "start:dev": "npm run dev",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "ts-node --transpile-only prisma/seed.ts",
    "generate": "ts-node scripts/generate-module.ts"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.0",
    "@nestjs/swagger": "^11.1.6",
    "@prisma/client": "^6.7.0",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express": "^5.1.0",
    "my-platform": "file:",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "react-toastify": "^11.0.5",
    "recharts": "^2.15.3",
    "uuid": "^11.1.0",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.3",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "prisma": "^6.7.0",
    "ts-node": "^10.9.2"
  }
}
